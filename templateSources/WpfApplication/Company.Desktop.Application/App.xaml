             xmlns:local="clr-namespace:Company.Desktop.Application"
             xmlns:controls="clr-namespace:Company.Desktop.ViewModels.Controls;assembly=Company.Desktop.ViewModels"
             xmlns:controls1="clr-namespace:Company.Desktop.Application.Views.Controls"
             StartupUri="Views/Windows/Splash.xaml">
    <Application.Resources>
		<ResourceDictionary>
